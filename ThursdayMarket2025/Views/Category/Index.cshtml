@model List<Category>


	
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>
					Category Name
				</th>

				<th>
					Categoroy Display Order
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
					<td> @item.Name </td>
					<td> @item.Id </td>
				</tr>
			}
		</tbody>
	</table>

